<script>
    document.addEventListener('DOMContentLoaded', function () {
        function handleSelectOperation(selectElement) {
            const yapilacak_tarih = document.getElementById('yapilacak_tarih');
            const km = document.getElementById('km');
            const selectedValue = selectElement.value;
            console.log(selectedValue);

            if (selectedValue === '3') {
                yapilacak_tarih.style.display = 'none';
                km.style.display = 'block';
            } else if (selectedValue === '2' || selectedValue === '1') {
                yapilacak_tarih.style.display = 'flex';
                km.style.display = 'none';
            }
        }
    })
</script>

